<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/homeFragment">
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.jeffaccount.ui.home.HomeFragment"
        android:label=""
        tools:layout="@layout/home_fragment">
        <action
            android:id="@+id/action_homeFragment_to_customerFragment"
            app:destination="@id/customerFragment" />
        <action
            android:id="@+id/action_homeFragment_to_supplierFragment"
            app:destination="@id/supplierFragment" />
        <action
            android:id="@+id/action_homeFragment_to_quotationFragment"
            app:destination="@id/quotationFragment" />
        <action
            android:id="@+id/action_homeFragment_to_purchaseFragment"
            app:destination="@id/purchaseFragment" />
        <action
            android:id="@+id/action_homeFragment_to_timeSheetFragment"
            app:destination="@id/timeSheetFragment" />
        <action
            android:id="@+id/action_homeFragment_to_invoiceFragment"
            app:destination="@id/invoiceFragment" />
        <action
            android:id="@+id/action_homeFragment_to_createWorkSheetFragment"
            app:destination="@id/createWorkSheetFragment"
            app:launchSingleTop="false"
            app:popUpToInclusive="false" />
        <argument
            android:name="filepath"
            app:argType="string"
            app:nullable="true" />
        <action
            android:id="@+id/action_homeFragment_to_addCompanyFragment"
            app:destination="@id/addCompanyFragment" />
        <action
            android:id="@+id/action_homeFragment_to_changePasswordFragment"
            app:destination="@id/changePasswordFragment" />
        <action
            android:id="@+id/action_homeFragment_to_aboutFragment"
            app:destination="@id/aboutFragment" />
    </fragment>
    <fragment
        android:id="@+id/aboutFragment"
        android:name="com.example.jeffaccount.ui.about.AboutFragment"
        android:label="" />
    <fragment
        android:id="@+id/contactUsFragment"
        android:name="com.example.jeffaccount.ui.contactus.ContactUsFragment"
        android:label=""
        tools:layout="@layout/contact_us_fragment" />
    <fragment
        android:id="@+id/faqFragment"
        android:name="com.example.jeffaccount.ui.faq.FaqFragment"
        android:label=""
        tools:layout="@layout/faq_fragment" />
    <fragment
        android:id="@+id/addCustomer"
        android:name="com.example.jeffaccount.ui.home.customer.AddCustomerFragment"
        android:label=""
        tools:layout="@layout/add_customer_fragment">
        <action
            android:id="@+id/action_addCustomer_to_customerFragment"
            app:destination="@id/customerFragment"
            app:popUpTo="@id/homeFragment"
            app:popUpToInclusive="false" />
        <argument
            android:name="post"
            app:argType="com.example.jeffaccount.model.Post" />
        <argument
            android:name="add"
            app:argType="string" />
        <action
            android:id="@+id/action_addCustomer_to_addQuotationFragment"
            app:destination="@id/addQuotationFragment"
            app:popUpTo="@id/addQuotationFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_addCustomer_to_addInvoiceFragment"
            app:destination="@id/addInvoiceFragment" />
    </fragment>
    <fragment
        android:id="@+id/addSupplierFragment"
        android:name="com.example.jeffaccount.ui.home.supplier.AddSupplierFragment"
        android:label=""
        tools:layout="@layout/add_supplier_fragment" >
        <action
            android:id="@+id/action_addSupplierFragment_to_supplierFragment"
            app:destination="@id/supplierFragment"
            app:popUpTo="@id/homeFragment" />
        <argument
            android:name="supplier"
            app:argType="com.example.jeffaccount.model.SupPost" />
        <argument
            android:name="update"
            app:argType="string" />
        <action
            android:id="@+id/action_addSupplierFragment_to_addPurchaseFragment"
            app:destination="@id/addPurchaseFragment" />
    </fragment>
    <fragment
        android:id="@+id/addQuotationFragment"
        android:name="com.example.jeffaccount.ui.home.quotation.AddQuotationFragment"
        android:label="" >
        <action
            android:id="@+id/action_addQuotationFragment_to_quotationFragment"
            app:destination="@id/quotationFragment"
            app:popUpTo="@id/homeFragment" />
        <argument
            android:name="quotation_item"
            app:argType="com.example.jeffaccount.model.QuotationPost"
            app:nullable="true" />
        <argument
            android:name="action_quotation"
            app:argType="string" />
        <action
            android:id="@+id/action_addQuotationFragment_to_addItemFragment"
            app:destination="@id/addItemFragment" />
        <argument
            android:name="item"
            app:argType="com.example.jeffaccount.network.ItemList"
            app:nullable="true" />
        <argument
            android:name="customer_item"
            app:argType="com.example.jeffaccount.model.Post"
            app:nullable="true" />
        <action
            android:id="@+id/action_addQuotationFragment_to_showPdfFragment"
            app:destination="@id/showPdfFragment" />
        <action
            android:id="@+id/action_addQuotationFragment_to_addInvoiceFragment"
            app:destination="@id/addInvoiceFragment" />
        <action
            android:id="@+id/action_addQuotationFragment_to_addPurchaseFragment"
            app:destination="@id/addPurchaseFragment"
            app:popUpTo="@id/addPurchaseFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_addQuotationFragment_to_addTimeSheetFragment"
            app:destination="@id/addTimeSheetFragment"
            app:popUpTo="@id/addTimeSheetFragment"
            app:popUpToInclusive="true" />
        <argument
            android:name="jobno"
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="quotationno"
            app:argType="string"
            app:nullable="true" />
        <action
            android:id="@+id/action_addQuotationFragment_to_createWorkSheetFragment"
            app:destination="@id/createWorkSheetFragment"
            app:popUpTo="@id/addQuotationFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/addPurchaseFragment"
        android:name="com.example.jeffaccount.ui.home.purchase.AddPurchaseFragment"
        android:label="" >
        <action
            android:id="@+id/action_addPurchaseFragment_to_purchaseFragment"
            app:destination="@id/purchaseFragment"
            app:popUpTo="@id/homeFragment" />
        <argument
            android:name="purchase_post"
            app:argType="com.example.jeffaccount.model.PurchasePost"
            app:nullable="true" />
        <argument
            android:name="action"
            app:argType="string" />
        <action
            android:id="@+id/action_addPurchaseFragment_to_addItemFragment"
            app:destination="@id/addItemFragment" />
        <argument
            android:name="company_item"
            app:argType="com.example.jeffaccount.model.ComPost"
            app:nullable="true" />
        <argument
            android:name="supplier_item"
            app:argType="com.example.jeffaccount.model.SupPost"
            app:nullable="true" />
        <action
            android:id="@+id/action_addPurchaseFragment_to_addQuotationFragment"
            app:destination="@id/addQuotationFragment"
            app:popUpTo="@id/addQuotationFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_addPurchaseFragment_to_addTimeSheetFragment"
            app:destination="@id/addTimeSheetFragment"
            app:popUpTo="@id/addTimeSheetFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_addPurchaseFragment_to_addInvoiceFragment"
            app:destination="@id/addInvoiceFragment"
            app:popUpTo="@id/addInvoiceFragment"
            app:popUpToInclusive="true" />
        <argument
            android:name="jobno"
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="quotationno"
            app:argType="string"
            app:nullable="true" />
        <action
            android:id="@+id/action_addPurchaseFragment_to_createWorkSheetFragment"
            app:destination="@id/createWorkSheetFragment" />
        <action
            android:id="@+id/action_addPurchaseFragment_to_searchFragment"
            app:destination="@id/searchFragment" />
        <action
            android:id="@+id/action_addPurchaseFragment_to_addSupplierFragment"
            app:destination="@id/addSupplierFragment" />
        <action
            android:id="@+id/action_addPurchaseFragment_to_addSupplierFragment2"
            app:destination="@id/addSupplierFragment" />
    </fragment>
    <fragment
        android:id="@+id/customerFragment"
        android:name="com.example.jeffaccount.ui.home.customer.CustomerFragment"
        android:label=""
        tools:layout="@layout/fragment_customer" >
        <action
            android:id="@+id/action_customerFragment_to_addCustomer"
            app:destination="@id/addCustomer" />
    </fragment>
    <fragment
        android:id="@+id/supplierFragment"
        android:name="com.example.jeffaccount.ui.home.supplier.SupplierFragment"
        android:label=""
        tools:layout="@layout/fragment_supplier" >
        <action
            android:id="@+id/action_supplierFragment_to_addSupplierFragment"
            app:destination="@id/addSupplierFragment" />
    </fragment>
    <fragment
        android:id="@+id/quotationFragment"
        android:name="com.example.jeffaccount.ui.home.quotation.QuotationFragment"
        android:label=""
        tools:layout="@layout/fragment_quotation" >
        <action
            android:id="@+id/action_quotationFragment_to_addQuotationFragment"
            app:destination="@id/addQuotationFragment" />
    </fragment>
    <fragment
        android:id="@+id/purchaseFragment"
        android:name="com.example.jeffaccount.ui.home.purchase.PurchaseFragment"
        android:label=""
        tools:layout="@layout/fragment_purchase" >
        <action
            android:id="@+id/action_purchaseFragment_to_addPurchaseFragment"
            app:destination="@id/addPurchaseFragment" />
    </fragment>
    <fragment
        android:id="@+id/addCompanyFragment"
        android:name="com.example.jeffaccount.ui.home.company.AddCompanyFragment"
        android:label="" >
        <action
            android:id="@+id/action_addCompanyFragment_to_addPurchaseFragment"
            app:destination="@id/addPurchaseFragment" />
        <action
            android:id="@+id/action_addCompanyFragment_to_homeFragment"
            app:destination="@id/homeFragment"
            app:popUpTo="@id/homeFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/timeSheetFragment"
        android:name="com.example.jeffaccount.ui.home.timeSheet.TimeSheetFragment"
        android:label=""
        tools:layout="@layout/time_sheet_fragment" >
        <action
            android:id="@+id/action_timeSheetFragment_to_addTimeSheetFragment"
            app:destination="@id/addTimeSheetFragment" />
    </fragment>
    <fragment
        android:id="@+id/addTimeSheetFragment"
        android:name="com.example.jeffaccount.ui.home.timeSheet.AddTimeSheetFragment"
        android:label=""
        tools:layout="@layout/fragment_add_time_sheet" >
        <action
            android:id="@+id/action_addTimeSheetFragment_to_timeSheetFragment"
            app:destination="@id/timeSheetFragment"
            app:popUpTo="@id/homeFragment"
            app:popUpToInclusive="false" />
        <argument
            android:name="time_sheet_item"
            app:argType="com.example.jeffaccount.model.TimeSheetPost"
            app:nullable="true" />
        <argument
            android:name="action"
            app:argType="string"
            app:nullable="true" />
        <action
            android:id="@+id/action_addTimeSheetFragment_to_addQuotationFragment"
            app:destination="@id/addQuotationFragment"
            app:popUpTo="@id/addQuotationFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_addTimeSheetFragment_to_addPurchaseFragment"
            app:destination="@id/addPurchaseFragment"
            app:popUpTo="@id/addPurchaseFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_addTimeSheetFragment_to_addInvoiceFragment"
            app:destination="@id/addInvoiceFragment"
            app:popUpTo="@id/addInvoiceFragment"
            app:popUpToInclusive="true" />
        <argument
            android:name="jobno"
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="quotationno"
            app:argType="string"
            app:nullable="true" />
        <action
            android:id="@+id/action_addTimeSheetFragment_to_createWorkSheetFragment"
            app:destination="@id/createWorkSheetFragment" />
    </fragment>
    <fragment
        android:id="@+id/addItemFragment"
        android:name="com.example.jeffaccount.ui.home.AddItemFragment"
        android:label=" "
        tools:layout="@layout/fragment_add_item" >
        <action
            android:id="@+id/action_addItemFragment_to_addPurchaseFragment"
            app:destination="@id/addPurchaseFragment"
            app:popUpTo="@id/purchaseFragment" />
        <action
            android:id="@+id/action_addItemFragment_to_addQuotationFragment"
            app:destination="@id/addQuotationFragment"
            app:popUpTo="@id/quotationFragment" />
        <argument
            android:name="itemlist"
            app:argType="com.example.jeffaccount.network.ItemList"
            app:nullable="true" />
    </fragment>
    <fragment
        android:id="@+id/showPdfFragment"
        android:name="com.example.jeffaccount.ui.home.ShowPdfFragment"
        android:label="fragment_show_pdf"
        tools:layout="@layout/fragment_show_pdf" >
        <argument
            android:name="path"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/invoiceFragment"
        android:name="com.example.jeffaccount.ui.home.invoice.InvoiceFragment"
        android:label="" >
        <action
            android:id="@+id/action_invoiceFragment_to_addInvoiceFragment"
            app:destination="@id/addInvoiceFragment" />
    </fragment>
    <fragment
        android:id="@+id/addInvoiceFragment"
        android:name="com.example.jeffaccount.ui.home.invoice.AddInvoiceFragment">
        <action
            android:id="@+id/action_addInvoiceFragment_to_invoiceFragment"
            app:destination="@id/invoiceFragment" />
        <argument
            android:name="action"
            app:argType="string" />
        <argument
            android:name="invoice"
            app:argType="com.example.jeffaccount.model.Invoice"
            app:nullable="true" />
        <argument
            android:name="customer"
            app:argType="com.example.jeffaccount.model.Post"
            app:nullable="true" />
        <argument
            android:name="quotation"
            app:argType="com.example.jeffaccount.model.QuotationPost"
            app:nullable="true" />
        <action
            android:id="@+id/action_addInvoiceFragment_to_addPurchaseFragment"
            app:destination="@id/addPurchaseFragment"
            app:popUpTo="@id/addPurchaseFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_addInvoiceFragment_to_addTimeSheetFragment"
            app:destination="@id/addTimeSheetFragment"
            app:popUpTo="@id/addTimeSheetFragment"
            app:popUpToInclusive="true" />
        <argument
            android:name="jobno"
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="quotationno"
            app:argType="string"
            app:nullable="true" />
        <action
            android:id="@+id/action_addInvoiceFragment_to_createWorkSheetFragment"
            app:destination="@id/createWorkSheetFragment" />
    </fragment>
    <fragment
        android:id="@+id/createWorkSheetFragment"
        android:name="com.example.jeffaccount.ui.home.worksheet.CreateWorkSheetFragment"
        android:label=" " >
        <action
            android:id="@+id/action_createWorkSheetFragment_to_homeFragment"
            app:destination="@id/homeFragment"
            app:popUpTo="@id/homeFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/changePasswordFragment"
        android:name="com.example.jeffaccount.ui.ChangePasswordFragment"
        android:label="ChangePasswordFragment" />
    <dialog
        android:id="@+id/searchCustomerBottomSheetFragment"
        android:name="com.example.jeffaccount.ui.home.quotation.SearchCustomerBottomSheetFragment"
        android:label="SearchCustomerBottomSheetFragment" >
        <argument
            android:name="type"
            app:argType="string" />
        <argument android:name="namelist"
            app:argType="com.example.jeffaccount.model.NameList" />
    </dialog>
    <fragment
        android:id="@+id/searchFragment"
        android:name="com.example.jeffaccount.ui.home.SearchFragment">
        <argument
            android:name="names"
            app:argType="com.example.jeffaccount.model.NameList"
            app:nullable="true" />
        <argument
            android:name="type"
            app:argType="string" />
        <action
            android:id="@+id/action_searchFragment_to_addSupplierFragment"
            app:destination="@id/addSupplierFragment" />
        <action
            android:id="@+id/action_searchFragment_to_addPurchaseFragment"
            app:destination="@id/addPurchaseFragment" />
    </fragment>
</navigation>